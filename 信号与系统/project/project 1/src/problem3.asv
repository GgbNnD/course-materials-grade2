%% 3.1
clc;
close all;
clear;

% read .wav
[y, Fs] = audioread('castanets44m.wav'); 

% draw the wave
t = (0:length(y)-1)/Fs; 
plot(t, y);
xlabel('Time (s)');
ylabel('Amplitude');

% play the .wav
sound(y, Fs); 

